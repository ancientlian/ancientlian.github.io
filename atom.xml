  <entry>
    <title>Django配置mysql</title>
    <link href="http://yoursite.com/2018/12/19/Django%E4%B8%AD%E9%85%8D%E7%BD%AEmysql/"/>
    <id>http://yoursite.com/2018/12/19/Django中配置mysql/</id>
