keyword&quot;&gt;return&lt;/span&gt; render(request , &lt;span class=&quot;string&quot;&gt;&#39;booktest/index.html&#39;&lt;/span&gt; , &amp;#123;&lt;span class=&quot;string&quot;&gt;&#39;books&#39;&lt;/span&gt;:books&amp;#125;)&lt;/span&gt;&lt;br&gt;&lt;/pre&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;/figure&gt;
    
